# Environment variables for WiHy UI
# Copy this file to .env.local and fill in your actual values

# Azure Configuration
AZURE_STATIC_WEB_APPS_API_TOKEN=your_azure_static_web_apps_token_here

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-domain.azurewebsites.net/api
REACT_APP_ENVIRONMENT=production

# Authentication Providers
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_MICROSOFT_CLIENT_ID=your_microsoft_client_id
REACT_APP_APPLE_CLIENT_ID=your_apple_client_id
REACT_APP_FACEBOOK_CLIENT_ID=your_facebook_client_id

# OpenFoodFacts API
REACT_APP_OPENFOODFACTS_API_URL=https://world.openfoodfacts.org

# Health API Configuration
REACT_APP_HEALTH_API_URL=https://your-health-api.azurewebsites.net
REACT_APP_NEWS_API_KEY=your_news_api_key

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CACHING=true
REACT_APP_DEBUG_MODE=false

