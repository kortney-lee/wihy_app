# Environment Variables for Google Cloud Deployment

# Copy this file to .env.local and fill in your actual values

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=wihy-ai
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json

# React App Configuration
REACT_APP_API_BASE_URL=https://nutrition-analysis-[hash]-uc.a.run.app
REACT_APP_ENVIRONMENT=production

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=wihy-ai.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=wihy-ai
REACT_APP_FIREBASE_STORAGE_BUCKET=wihy-ai.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_app_id_here

# Cloud Run Configuration
CLOUD_RUN_SERVICE_NAME=wihy-frontend
CLOUD_RUN_REGION=us-central1

# Cloud Functions Configuration
NUTRITION_FUNCTION_URL=https://us-central1-wihy-health-app.cloudfunctions.net/nutrition-analysis
RESEARCH_FUNCTION_URL=https://us-central1-wihy-health-app.cloudfunctions.net/research-api

# Storage Configuration
STORAGE_BUCKET=wihy-user-uploads
CDN_BASE_URL=https://storage.googleapis.com/wihy-user-uploads

# AI/ML Configuration
VERTEX_AI_LOCATION=us-central1
VISION_API_ENABLED=true

# Monitoring and Logging
ENABLE_CLOUD_LOGGING=true
ENABLE_CLOUD_MONITORING=true
LOG_LEVEL=info

# Security Configuration
CORS_ALLOWED_ORIGINS=https://wihy-health-app.web.app,https://app.wihy.ai
SESSION_SECRET=your_session_secret_here

# Third-party APIs (if any)
NUTRITION_API_KEY=your_nutrition_api_key_here
RESEARCH_API_KEY=your_research_api_key_here

# Development/Staging URLs (for non-production deployments)
REACT_APP_STAGING_API_URL=https://nutrition-analysis-staging-[hash]-uc.a.run.app
REACT_APP_DEV_API_URL=http://localhost:8080